{
  "name": "islin-crx",
  "description": "代码覆盖率上传插件",
  "version": "0.0.6",
  "manifest_version": 3,
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["interceptor.js"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-scripts.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "islin-crx",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}